{
  "name": "react-material-formutil",
  "author": "qiqiboy",
  "version": "1.0.0",
  "private": true,
  "vendor": [
    "react",
    "react-dom",
    "react-formutil"
  ],
  "noRewrite": true,
  "proxy": null,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:dev": "node scripts/build.js --dev",
    "pack": "npm run build",
    "count": "node scripts/count.js",
    "serve": "node scripts/serve.js",
    "count:js": "node scripts/count.js --js",
    "i18n-scan": "node scripts/i18n.js --scan",
    "i18n-read": "node scripts/i18n.js --read",
    "test": "node scripts/test.js",
    "tsc": "node -e \"require('fs-extra').outputJsonSync('.git-tsconfig.json',{ extends: './tsconfig.json', include: ['*.d.ts', 'app/utils/i18n/*'].concat(process.env.StagedFiles.split(/\\n+/)) })\" && echo 'TS checking...\\n' && tsc -p .git-tsconfig.json --checkJs false"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "./scripts/config/eslintrc.js"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "parser": "babylon",
    "trailingComma": "none",
    "semi": true,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "tabWidth": 2
        }
      }
    ],
    "arrowParens": "avoid",
    "bracketSameLine": true
  },
  "lint-staged": {
    "{app,static,tests}/**/*.{js,jsx,mjs,css,scss,less,json,ts}": [
      "node_modules/.bin/prettier --write"
    ]
  },
  "dependencies": {
    "@date-io/moment": "^0.0.2",
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "material-ui-pickers": "^2.0.2",
    "moment": "^2.22.2",
    "normalize.css": "5.0.0",
    "prop-types": "15.6.2",
    "raf-dom": "1.1.2",
    "react": "latest",
    "react-animated-router": "0.1.11",
    "react-awesome-snippets": "0.0.22",
    "react-dom": "latest",
    "react-formutil": "latest",
    "react-router-dom": "4.2.2",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "2.4.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "address": "1.0.2",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-preset-react-app": "3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "check-dependencies": "1.0.1",
    "classlist-polyfill": "1.2.0",
    "core-js": "2.5.1",
    "css-loader": "0.28.7",
    "detect-port": "1.2.1",
    "directory-named-webpack-plugin": "2.2.3",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "glob": "7.1.2",
    "html-loader": "0.5.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "inline-chunk-manifest-html-webpack-plugin": "2.0.0",
    "jest": "20.0.4",
    "less": "2.7.0",
    "less-loader": "4.0.5",
    "lint-staged": "5.0.0",
    "node-sass": "4.6.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "1.8.2",
    "react-hot-loader": "4.1.0",
    "rsync": "0.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "1.0.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=development lint-staged && export StagedFiles=$(git diff --name-only --diff-filter AM --relative --staged | grep -E '.tsx?$') && if [ -n \"$StagedFiles\" ]; then npm run tsc; fi",
      "commit-msg": "node_modules/.bin/commitlint --edit $HUSKY_GIT_PARAMS",
      "pre-push": "CF=$(git diff --diff-filter AM --name-only @{u}..) || CF=$(git diff --diff-filter AM --name-only origin/master...HEAD); FILES=$(echo \"$CF\" | grep -E '^app/.*\\.m?[jt]sx?$'); if [ -n \"$FILES\" ]; then NODE_ENV=production node_modules/.bin/eslint $FILES --max-warnings 0; fi"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ],
      "scope-case": [
        0
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "ie >= 10"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0",
    "tiger-new": "7.3.14"
  }
}
